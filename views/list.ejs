<%- include('layouts/header') %> 



<body>

    <div class="box">
        <h1 id="heading"><%= listTitle %> </h1>
    </div>
    

    <div class="box">
        
            <% new_entry.forEach(element => { %>
                <div class="item">
                    <input type="checkbox" name="" id="">
                    <p class="item"><%= element %> </p>
                </div>
                
            <% }) %>

            
                <form class="item" action="/" method="post">
                    <input type="text" name="todo_entry" id="" placeholder="New Item" required autocomplete="off">
                    <button type="submit" name="list" value=<%= listTitle %> >+</button>
                </form>
            
        
    </div>
    

    <%- include('layouts/footer') %> 
        
  